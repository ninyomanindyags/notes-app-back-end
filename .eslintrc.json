{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": 0,
    "no-trailing-spaces": 0,
    "no-multiple-empty-lines": 0,
    "eol-last": 0,
    "import/no-unresolved": 0,
    "no-underscore-dangle": "off",
    "import/no-extraneous-dependencies": "off",
    "no-unused-vars": ["warn", { "args": "none" }]
 }
}
